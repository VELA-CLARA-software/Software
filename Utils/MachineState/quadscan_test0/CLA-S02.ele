	use_beamline = CLA-S02
	centroid = %s.cen
	sigma = %s.sig
	default_order = 3
	p_central = 60.07192218652758
	always_change_p0 = 1
&end
&run_control
	n_steps = 1
	n_passes = 1
&end
&twiss_output
	filename = %s.twi
	matched = 0
	output_at_each_step = 0
