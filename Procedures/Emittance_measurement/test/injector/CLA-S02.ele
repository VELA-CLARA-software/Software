&global_settings
	inhibit_fsync = 1
	mpi_io_read_buffer_size = 16777216
	mpi_io_write_buffer_size = 16777216
&end
&run_setup
	lattice = CLA-S02.lte
	use_beamline = CLA-S02
	centroid = %s.cen
	sigma = %s.sig
	default_order = 3
	p_central = 42.328481797599395
	always_change_p0 = 1
&end
&run_control
	n_steps = 1
	n_passes = 1
&end
&twiss_output
	filename = %s.twi
	matched = 0
	output_at_each_step = 0
	statistics = 1
	radiation_integrals = 1
	beta_x = 3.333810672808801
	alpha_x = -1.0244470827654324
	eta_x = 0.05674629705176189
	etap_x = 0.09256266388088354
	beta_y = 3.4727320429610917
	alpha_y = -1.0372013542153193
&end
&floor_coordinates
	filename = %s.flr
	X0 = 0
	Z0 = 3.37147
	theta0 = 0
&end
&matrix_output
	full_matrix_only = 1
	SDDS_output = %s.mat
	SDDS_output_order = 2
&end
&sdds_beam
	input = CLA-S02.sdds
	sample_interval = 1
&end
&track
&end
