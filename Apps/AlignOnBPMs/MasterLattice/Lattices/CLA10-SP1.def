global:
  total_charge: 0.25
  generatorFile: ASTRA/generator10.in
  SC_2D_Nrad: 30
  SC_2D_Nlong: 45
  SC_3D_Nxf: 8
  SC_3D_Nyf: 8
  SC_3D_Nzf: 32
  accuracy: 6
  initial_distribution: '../1k-250pC-76fsrms-1mm_TE09fixN12.ini'
  ASTRAsettings:
    N_red: 1
    EmitS: True
    PhaseS: True
    TrackS: True
    RefS: True
    Track_All: True
    Phase_Scan: False
    Auto_Phase: True
    check_ref_part: False
    Zphase: 1
    Zemit: 1050
    H_max: 0.0007
    H_min: 0.0007
    title: 'trial'
    run_no: 1
    Smooth_x: 2
    Smooth_y: 2
    Smooth_z: 2
    Max_scale: 0.1
    Max_count: 100
    cell_var: 2
    min_grid: 3.424657e-13
files:
  injector10:
    code: ASTRA
    charge:
      cathode: True
      space_charge_mode: 2D
      mirror_charge: True
    input:
      particle_definition: 'initial_distribution'
    output:
      start_element: CLA-LRG1-APER
      end_element: CLA-S02-APER-01
  s02:
    global_offset: $self.getElement('CLA-S02-APER-01','position_end')$
    global_rotation: 0
    code: ASTRA
    charge:
      cathode: False
      space_charge_mode: 3D
    input:
      particle_definition: $output['start_element']$.astra
    output:
      start_element: CLA-S02-APER-01
      end_element: CLA-C2V-MARK-01
  c2v:
    global_offset: $self.getElement('CLA-C2V-MAG-DIP-01','position_end')$
    starting_rotation: 0
    code: ASTRA
    charge:
      cathode: False
      space_charge_mode: 3D
    input:
      particle_definition: $output['start_element']$.astra
    output:
      start_element: CLA-C2V-MARK-01
      end_element: CLA-C2V-DIA-SCR-01-W
groups:
elements:
  filename: ['YAML/Injector10.yaml',
                  'YAML/S02.yaml',
                  'YAML/C2V.yaml']#,
                  #'YAML/SP1.yaml']
