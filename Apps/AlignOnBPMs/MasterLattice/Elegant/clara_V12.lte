!---------------Straight 1, cathode to linac 1------------------------------

CLA-LRG1-APER:MAXAMP, x_max = 17e-3, y_max = 17e-3, elliptical = 1

CLA-S01-APER:MAXAMP, x_max = 17e-3, y_max = 17e-3, elliptical = 1

CLA-S01-MAG-HVCOR-01:KICKER, l = 0.030, hkick = 0.0, vkick = 0.0, b2 = 0.0,&
			hcalibration = 1, vcalibration = 1,&
			edge_effects = 1, order = 2, steering = 1,&
			synch_rad = 1, isr = 1
CLA-S01-MAG-HVCOR-02:KICKER, l = 0.102, hkick = 0.0, vkick = 0.0, b2 = 0.0,&
			hcalibration = 1, vcalibration = 1,&
			edge_effects = 1, order = 2, steering = 1,&
			synch_rad = 1, isr = 1

! zero length as coincident with S1-DIA-BPM-01

CLA-LRG1-GUN:DRIFT, l = 0.32482

CLA-S01-VAC-VALV-01:DRIFT, l = 0.100

CLA-S01-LSR-LBX-01:DRIFT, l = 0.22288

CLA-S01-DIA-WCM-01:DRIFT, l = 0.050

CLA-S01-DIA-SCR-01-B:LSCDRIFT, bins = "lscbins",& 
			   high_frequency_cutoff0 = 0.2, high_frequency_cutoff1 = 0.25,&
			   interpolate = 1, radius_factor = 1.7, l = 0.043333
CLA-S01-DIA-SCR-01-W:WATCH, mode = coordinate, filename = CLA-S01-DIA-SCR-01.sdds
CLA-S01-DIA-SCR-01-A:LSCDRIFT, bins = "lscbins",& 
			   high_frequency_cutoff0 = 0.2, high_frequency_cutoff1 = 0.25,&
			   interpolate = 1, radius_factor = 1.7, l = 0.076667
CLA-S01-DIA-SCR-01: LINE =(CLA-S01-DIA-SCR-01-B, CLA-S01-DIA-SCR-01-W, CLA-S01-DIA-SCR-01-A)

CLA-S01-DIA-BPM-01:MONI, l = 0.108, co_fitpoint = 1

CLA-S01: LINE = (CLA-LRG1-APER, CLA-LRG1-GUN, CLA-S01-APER, &
	      CLA-S01-MAG-HVCOR-01, CLA-S01-VAC-VALV-01,&
	      CLA-S01-LSR-LBX-01, CLA-S01-DIA-WCM-01,&
	      CLA-S01-MAG-HVCOR-02, CLA-S01-DIA-BPM-01, CLA-S01-DIA-SCR-01)

