CLA-L01-CAV-B:LSCDRIFT, bins = "lscbins",& 
		     high_frequency_cutoff0 = 0.2, high_frequency_cutoff1 = 0.25,&
		     interpolate = 1, radius_factor = 1.7, l = 0.13587
CLA-L01-CAV:RFCW,freq = "frequency",&
		l = "lcell 61 *",&
		cell_length = "lcell",&
	  	volt = "volt_1 lcell * 61 *",&
		phase = "phase_1",&
		change_p0 = 1,&
		end1_focus = 1, end2_focus = 1,&
	        ZWAKEFILE = "Sz5um10mm.sdds",&
		TRWAKEFILE = "Sx5um10mm.sdds",&
	        TCOLUMN = "t", WZCOLUMN = "W",&
		WXCOLUMN = "W", WYCOLUMN = "W",&
		n_kicks = 61,&
		interpolate = 1,&
		smoothing = 1,&
		lsc = 1, lsc_bins = "lscbins", &
		lsc_high_frequency_cutoff0 = 0.2,&
		lsc_high_frequency_cutoff1 = 0.25, lsc_interpolate = 1,&
		lsc_radius_factor = 1.7,&
		body_focus_model = SRS
CLA-L01-CAV-A:LSCDRIFT, bins = "lscbins",& 
		     high_frequency_cutoff0 = 0.2, high_frequency_cutoff1 = 0.25,&
		     interpolate = 1, radius_factor = 1.7, l = 0.14457

CLA-L01: LINE =(CLA-L01-APER, CLA-L01-CAV-B, CLA-L01-CAV, CLA-L01-CAV-A)

